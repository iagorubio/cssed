		 <style name="FUNCTION" fore="0x993300" back="0xffffff" bold="false" />
		 <style name="NUMBER" fore="0x7b207f" back="0xffffff" bold="false" />
		 <style name="UNIT" fore="0x9b671e" back="0xffffff" bold="false" />
		 <style name="COLOR" fore="0x6600" back="0xffffff" bold="false" />
		 <style name="HEXACOLOR" fore="0x6600" back="0xffffff" bold="false" />
		 <style name="ATTR_MATCH" fore="0x4444" back="0xffffff" bold="false" />
		 <style name="LANGUAGE" fore="0x990000" back="0xffffff" bold="false" />
